local ScreenGui = Instance.new("ScreenGui")
local GlockFrame = Instance.new("Frame")
local GlockLabel = Instance.new("TextLabel")
local GlockColors = Instance.new("Frame")
local whitecolor = Instance.new("TextButton")
local blackcolor = Instance.new("TextButton")
local red = Instance.new("TextButton")
local green = Instance.new("TextButton")
local blue = Instance.new("TextButton")
local yellow = Instance.new("TextButton")
local purple = Instance.new("TextButton")
local pink = Instance.new("TextButton")
local ShottyFrame = Instance.new("Frame")
local ShottyLabel = Instance.new("TextLabel")
