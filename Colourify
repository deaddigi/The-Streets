ScreenGui.Parent = game.CoreGui

GlockFrame.Name = "GlockFrame"
GlockFrame.Parent = ScreenGui
GlockFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
GlockFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
GlockFrame.Position = UDim2.new(0.785888076, 0, 0.544973493, 0)
GlockFrame.Size = UDim2.new(0, 157, 0, 247)
GlockFrame.Visible = true

GlockLabel.Name = "GlockLabel"
GlockLabel.Parent = GlockFrame
GlockLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
GlockLabel.BackgroundTransparency = 1.000
GlockLabel.Size = UDim2.new(0, 157, 0, 25)
GlockLabel.Font = Enum.Font.RobotoMono
GlockLabel.Text = "Glock"
GlockLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
GlockLabel.TextSize = 14.000

GlockColors.Name = "GlockColors"
GlockColors.Parent = GlockFrame
GlockColors.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
GlockColors.BorderColor3 = Color3.fromRGB(0, 0, 0)
GlockColors.Position = UDim2.new(0.0509554148, 0, 0.101214573, 0)
GlockColors.Size = UDim2.new(0, 141, 0, 213)

whitecolor.Name = "whitecolor"
whitecolor.Parent = GlockColors
whitecolor.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
whitecolor.BorderColor3 = Color3.fromRGB(0, 0, 0)
whitecolor.Position = UDim2.new(0.0709220171, 0, 0.0657277107, 0)
whitecolor.Size = UDim2.new(0, 31, 0, 29)
whitecolor.Font = Enum.Font.SourceSans
whitecolor.Text = " "
whitecolor.TextColor3 = Color3.fromRGB(0, 0, 0)
whitecolor.TextSize = 14.000
whitecolor.MouseButton1Click:Connect(function()
    local getc = game.Players.LocalPlayer.Backpack.Glock:GetChildren()
    for i,v in pairs(getc) do
        if v.Name == "Union" then
            v.UsePartColor = true 
            wait()
            v.Material = "Neon"
            v.Color = Color3.fromRGB(255, 255, 255)
			GlockFrame.Visible = false
        end
    end
end)

